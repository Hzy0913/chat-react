{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": ["transform-runtime", "transform-decorators-legacy", "react-hot-loader/babel", ["import", {
    "libraryName": "antd-mobile",
    "style": "css"
  }]]
}
